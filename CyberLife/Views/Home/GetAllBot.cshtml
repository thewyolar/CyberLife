@model IList<PerceptronModel>
@{
    Layout = null;
}
<div><button id="loading" onClick="loadingBot()">loading</button></div>
@for (int i = 0; i < Model.Count; i++)
{
    <div id="@Model[i].Id" class="botLoading" style="background: rgb(@Model[i].RGB[0] @Model[i].RGB[1] @Model[i].RGB[2])"> @Model[i].Name</div>
}